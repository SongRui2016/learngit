<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="user" class="com.frame.sprinContainer.beans.UserBean">
		<property name="userId" value=1></property>
		<property name="userName" value="小张"></property>
		<property name="userGender" value="男"></property>
		<property name="userAge" value=12></property>
	</bean>

	<bean id="userServiceImpl" class="com.frame.sprinContainer.service.impl.UserServiceImpl"
		autowire="constructor"></bean>
		
     </bean>
     
</beans>
